using System;

class OddOrEven 
{
    static void Main(string[] args) 
    {
        Console.Write("Enter a number: ");
        int num = Convert.ToInt32(Console.ReadLine());

        if (num % 2 == 0) 
        {
            Console.WriteLine("Even number");
        } 
        else 
        {
            Console.WriteLine("Odd number");
        }
    }
}
